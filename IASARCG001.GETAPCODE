PARAMETERS: 
 INTEGER SELITEM,
 STRING CMBCODES,
 INTEGER PTYPE;

OBJECT: 
 STRING TMPCODES,
 STRING SELCODE,
 STRING TMPSTR,
 INTEGER COUNT;

TMPCODES = CMBCODES;

IF PTYPE == 1 THEN
	TMPCODES = CMBCAPTION;
ENDIF;

SELCODE = SPACE;
COUNT = 0;

PARSE TMPCODES INTO TMPSTR DELIMITER SEMICOLON 
BEGIN

	IF COUNT == SELITEM THEN
		SELCODE = TMPSTR;
		BREAK;
	ENDIF;

	COUNT = COUNT + 1;
ENDPARSE;

RETURN SELCODE;
