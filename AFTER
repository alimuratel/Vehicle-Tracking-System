IF ZOOMID == 0 THEN
	SET OK TO HIDE;
	SET OK TO DISABLE;
ELSE
	SET NEW TO HIDE;
	SET CHANGE TO HIDE;
	SET SHOW TO HIDE;
	SET NEW TO DISABLE;
	SET CHANGE TO DISABLE;
	SET SHOW TO DISABLE;
ENDIF;

THIS.F10();
SET TBLARAC TO RESIZE;
THIS.SEARCH();
SETSTYLE BACKCOLOR KOYUOKK TO LBLOKKTESIS;
SETSTYLE BACKCOLOR KOYUOKT TO LBLOKTTESIS;
SETSTYLE BACKCOLOR KOYUOKE TO LBLOKETESIS;
SETSTYLE BACKCOLOR KOYUOKS TO LBLOKSTESIS;
SETSTYLE BACKCOLOR KOYUOKV TO LBLOKVTESIS;
