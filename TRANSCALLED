
SWITCH CALLEDFROMWHERE 
CASE 'SALT01':
	/*Satış ekranından gelerek ek belgeleri görüntüleme İLK*/
	THIS.F10();
	MOVE P2 TO SCSASINO;
	THIS.SEARCH();
	MODI = 2;
	SELECT * 
		FROM IASARACHEAD OKC 
		WHERE OKC.CLIENT = SYS_CLIENT 
			AND OKC.DOCNUM = TBLARAC_DOCNUM;

	CUSTOMER = IASARACHEAD_CUSTOMER;
	CALL DIALOG IASARCG001;
	/*Satış ekranından gelerek ek belgeleri görüntüleme SON*/
CASE 'IASQLT06':
	THIS.F10();
	MOVE '01.01.1975' TO SCFROMDATE;
	MOVE P2 TO SCDOCNUM;
	MOVE P3 TO SCSASINO;
	THIS.SEARCH();

	IF TBLARAC_ROWCOUNT == 1 THEN
		MODI = 2;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CALL DIALOG IASARCG001;
		SHUTDOWN;
	ENDIF;

	/**/
CASE 'IASQLT01':
	THIS.F10();
	MOVE P2 TO SCDOCNUM;
	SCFROMDATE = '01.01.1975';
	THIS.SEARCH();

	IF TBLARAC_ROWCOUNT == 1 THEN
		MODI = 2;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CALL DIALOG IASARCG001;
		SHUTDOWN;
	ENDIF;

	/**/
CASE 'IASPRD01':
	THIS.F10();
	MOVE P2 TO SCDOCNUM;
	SCFROMDATE = '01.01.1975';
	THIS.SEARCH();

	IF TBLARAC_ROWCOUNT == 1 THEN
		MODI = 2;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CALL DIALOG IASARCG001;
		SHUTDOWN;
	ENDIF;

	/**/
CASE  'IASSAL01':

	IF P3 == 'KAMYONAC' THEN
		THIS.F10();
		MOVE P2 TO SCDOCNUM;
		SCFROMDATE = '01.01.1975';
		THIS.SEARCH();

		IF LREC.ADDLOCK('AR', TBLARAC_DOCNUM) == 0 THEN
			RETURN;
		ENDIF;

		MODI = 1;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CALL DIALOG IASARCG001;
	ELSE
		THIS.F10();
		MOVE P2 TO SCDOCNUM;
		SCFROMDATE = '01.01.1975';
		THIS.SEARCH();
		MODI = 2;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CUSTOMER = IASARACHEAD_CUSTOMER;
		CALL DIALOG IASARCG001;
	ENDIF;

CASE 'IASDLV01':
	THIS.F10();
	MOVE '01.01.1975' TO SCFROMDATE;
	MOVE P2 TO SCSASINO;
	THIS.SEARCH();

	IF TBLARAC_ROWCOUNT == 1 THEN
		MODI = 2;
		SELECT * 
			FROM IASARACHEAD OKC 
			WHERE OKC.CLIENT = SYS_CLIENT 
				AND OKC.DOCNUM = TBLARAC_DOCNUM;

		CUSTOMER = IASARACHEAD_CUSTOMER;
		CALL DIALOG IASARCG001;
		SHUTDOWN;
	ENDIF;

ENDSWITCH;

