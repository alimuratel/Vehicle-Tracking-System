PARAMETERS: 
 STRING COM,
 INTEGER INCLUDEKEY,
 INTEGER FIRSTEMPTY,
 STRING OTHERKEY,
 STRING EMPTYSTR;

SELECT USTYAPITIPI ,STEXT 
	FROM IASARC040 
	WHERE  TIPI = IASARACHEAD_ARACTIPI 
	ORDERBY USTYAPITIPI 
	INTO TBLTESIS;

THIS.CREATECAPTION('TBLTESIS','USTYAPITIPI','STEXT',INCLUDEKEY,FIRSTEMPTY,OTHERKEY,EMPTYSTR,COMBOTYPE,COLUMNTEXT);
RETURN CMBCAPTION;
